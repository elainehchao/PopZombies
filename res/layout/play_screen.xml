<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context="edu.OOSE.cs.jhu.group2.PopZombiesUI.StillImageActivity"
    xmlns:custom="http://schemas.android.com/apk/res/edu.OOSE.cs.jhu.group2.PopZombiesModel"
    >

  
  
  <edu.OOSE.cs.jhu.group2.PopZombiesUI.GameView
      	android:id="@+id/background"
	    android:background="@drawable/iter2game"
      	android:layout_width="fill_parent"
    	android:layout_height="wrap_content"
    	android:adjustViewBounds="true"
    	
      />
  
  
  <Button
        android:id="@+id/button2"
        android:background="@drawable/white"
        android:layout_width="116dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="60dp"
        android:onClick="backToMenu"
        android:text="Back To Menu" />
  
  <Button
        android:id="@+id/button3"
        android:background="@drawable/white"
        android:layout_width="116dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="10dp"
        android:onClick="pause"
        android:text="PAUSE" />

</RelativeLayout>